#!/bin/bash
git fetch upstream && \
    git checkout stable/1.10.x && git merge --ff-only upstream/stable/1.10.x && \
    git checkout stable/1.9.x && git merge --ff-only upstream/stable/1.9.x && \
    git checkout stable/1.8.x && git merge --ff-only upstream/stable/1.8.x && \
    git checkout master && git merge --ff-only upstream/master
